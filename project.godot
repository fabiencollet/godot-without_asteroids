}

[application]

config/name="Without_Asteroids"
config/description="2D Multiplayer local space shooter"
run/main_scene="res://Scenes/Splash_Screen.tscn"
boot_splash/image="res://Assets/Menu/bg_splashscreen.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://Assets/Menu/icon.ico"

[autoload]

Globals="*res://Scripts/Globals.gd"
MusicManager="*res://Singletons/MusicManager.tscn"

[display]

window/size/width=1200
window/size/height=900
window/size/borderless=true
window/size/fullscreen=true
window/size/always_on_top=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

